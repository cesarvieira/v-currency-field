{
  "name": "@cesarv/v-currency-field",
  "version": "1.0.2",
  "description": "A Vue 3 currency input component for Vuetify 3 with TypeScript support",
  "main": "dist/v-currency-field.umd.js",
  "module": "dist/v-currency-field.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/v-currency-field.mjs",
      "require": "./dist/v-currency-field.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/v-currency-field.css": "./dist/v-currency-field.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "build": "vite build",
    "preversion": "npm run lint && npm run type-check"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuetify",
    "vuetify3",
    "currency",
    "currency-field",
    "typescript",
    "component",
    "vue-component"
  ],
  "author": {
    "name": "Cesar Vieira",
    "email": "cesarvieira.dev@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cesarvieira/v-currency-field.git"
  },
  "bugs": {
    "url": "https://github.com/cesarvieira/v-currency-field/issues"
  },
  "homepage": "https://github.com/cesarvieira/v-currency-field#readme",
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.2.1"
  },
  "peerDependencies": {
    "vue": "^3.5.26",
    "vuetify": "^3.11.5"
  },
  "dependencies": {
    "vue-currency-input": "^3.2.2"
  }
}
